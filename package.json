{
  "name": "dotfiles",
  "version": "0.0.4",
  "license": "MIT",
  "author": "Nicholas Berlette <nick@berlette.com>",
  "description": "Dotfiles configuration for Gitpod and local development",
  "repository": "nberlette/dotfiles",
  "bugs": "https://github.com/nberlette/dotfiles/issues",
  "readme": "https://github.com/nberlette/dotfiles#readme",
  "homepage": "https://dotfiles.ml",
  "keywords": [
    "dotfiles",
    "gitpod",
    "podfiles",
    "config"
  ],
  "prettier": "@brlt/prettier",
  "main": "./install.sh",
  "packageManager": "pnpm@6.32.6",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "./install.sh",
    "test": "pnpm lint && pnpm check",
    "check": "shellcheck --color=always --shell=bash -x .{path,functions,bash*,exports}",
    "lint": "shfmt -ci -bn -sr -kp -fn -ln=bash -i 4 -w -s ",
    "format:check": "prettier --config package.json --no-error-on-unmatched-pattern --parser sh --prettier-plugin-sh --check \"**/*\" \"**/.*\"",
    "format:fix": "prettier --config package.json --no-error-on-unmatched-pattern --parser sh --plugin prettier-plugin-sh --write \"**/*\" \"**/.*\""
  },
  "devDependencies": {
    "@antfu/ni": "^0.13.2",
    "@brlt/prettier": "^1.6.4",
    "pnpm": "^6.32.4",
    "prettier": "^2.6.1",
    "prettier-plugin-sh": "^0.8.1",
    "shellcheck": "^1.1.0"
  }
}
